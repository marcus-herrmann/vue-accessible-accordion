{
  "name": "vue-accessible-accordion",
  "version": "0.3",
  "description": "Behavioural and accessible accordion component",
  "contributors": [
    "Voorhoede",
    "marcus-herrmann"
  ],
  "private": false,
  "license": "MIT",
  "main": "./dist/vueaccordion.common.js",
  "engines": {
    "node": ">= 10.16.0"
  },
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name vueaccordion ./src/app.vue",
    "lint": "vue-cli-service lint"
  },
  "homepage": "https://github.com/voorhoede/vue-accordion#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/voorhoede/vue-accordion"
  },
  "bugs": {
    "url": "https://github.com/voorhoede/vue-accordion/issues"
  },
  "keywords": [
    "accordion",
    "expandable",
    "vue",
    "vuejs",
    "accessible"
  ],
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "dependencies": {
    "core-js": "^3.6.5",
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0-0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
